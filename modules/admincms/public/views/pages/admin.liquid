<h2> Module example - Admin </h2>

<div class="row">
  <div class="col-xs-12 col-lg-6">
    <div class="card">
      <div class="card-header">
        <h3> Models </h3>
      </div>
      <div class="card-body">
        {%- graphql c = "modules/admincms/get_models" -%}
        <table class="table table-sm" data-result="models">
          <thead>
            <tr>
              <th>Name</th>
              <th>Properties</th>
            </tr>
          </thead>
          <tbody>
            {% for model in c.models.results %}
              <tr>
                <td>{{ model.name }}</td>
                <td>{{ model.properties }}</td>
              </tr>
            {% endfor %}
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-lg-6">
    <div class="card">
      <div class="card-header">
        <h3>Pages</h3>
      </div>

      <div class="card-body">
        {%- graphql c = "modules/admincms/get_pages" -%}
        <table class="table table-sm" data-result="pages">
          <thead>
            <tr>
              <th>Slug</th>
              <th>Format</th>
            </tr>
          </thead>
          <tbody>
            {% for page in c.admin_pages.results %}
              <tr>
                <td>{{ page.slug }}</td>
                <td>{{ page.format }}</td>
              </tr>
            {% endfor %}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<div class="row mt-5">
  <div class="col-12">
    <h3>Instance assets, including this module</h3>

    <table class="table table-sm table-striped" data-result="assets">
      <thead>
        <tr>
          <th>Asset path</th>
          <th>Resolved url</th>
        </tr>
      </thead>
      <tbody>
        {% for asset in asset_url %}
          <tr>
            <td>{{ asset[0] }}</td>
            <td>{{ asset[1] }}</td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
</div>
